        tools:layout="@layout/viewpager_follow" />
    <fragment
        android:id="@+id/messageFragment"
        android:name="com.joad.sinkystar.Fragment.message.fragment.MessageFragment"
        android:label="MessageFragment"
        tools:layout="@layout/fragment_message">
        <action
            android:id="@+id/action_messageFragment_to_chatFragment2"
            app:destination="@id/chatFragment2"
            app:enterAnim="@anim/animate_slide_left_enter"
            app:exitAnim="@anim/animate_slide_left_exit" />
    </fragment>
    <fragment
        android:id="@+id/chatFragment2"
        android:name="com.joad.sinkystar.Fragment.message.fragment.ChatFragment"
        android:label="chatFragment"
        tools:layout="@layout/fragment_chat">

    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.joad.sinkystar.Fragment.search.fragment.SearchFragment"
        android:label="SearchFragment"
        tools:layout="@layout/fragment_search">
        <action
            android:id="@+id/action_searchFragment_to_userProfileFragment"
            app:destination="@id/userProfileFragment" />
        <action
            android:id="@+id/action_searchFragment_to_video_user_view_frag"
            app:destination="@id/video_user_view_frag" />
        <action
            android:id="@+id/action_searchFragment_to_searchFragment2"
            app:destination="@id/searchFragment2"
            app:enterAnim="@anim/animate_slide_up_enter"
            app:exitAnim="@anim/animate_slide_up_exit"
            app:popEnterAnim="@anim/animate_slide_down_enter"
            app:popExitAnim="@anim/animate_slide_down_exit" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment2"
        android:name="com.joad.sinkystar.Fragment.search.fragment.SearchFragment2"
